@article{engelberg_anomalies_2015,
abstract = {Using a sample of 97 stock return anomalies documented in published studies, we find that anomaly returns are 7 times higher on earnings announcement days and 2 times higher on corporate news days. The effects are similar on both the long and short sides, and they survive adjustments for risk exposure and data mining. Moreover, anomaly signals predict errors in analysts' earnings forecasts — analysts' forecasts are systematically too low for anomaly-longs and too high for anomaly-shorts. Taken together, our results support the view that anomaly returns are the result of biased expectations, which are at least partially corrected upon news arrival.},
author = {Engelberg, Joseph and McLean, R David and Pontiff, Jeffrey},
doi = {10.2139/ssrn.2631228},
